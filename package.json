    "test": "BABEL_ENV=jest jest --config jest.config.js",
    "build": "webpack --debug --config webpack.config/development.js",
    "watch": "webpack --debug --config webpack.config/development.js --watch &",
    "production": "webpack -p --config webpack.config/production.js",
    "clean": "rm public/bundles/*",
    "firebase": "firebase serve --only functions,hosting",
    "flow": "flow || exit 0",
    "flow-typed": "flow-typed",
    "killServer": "sudo kill $(sudo lsof -t -i:8080)",
